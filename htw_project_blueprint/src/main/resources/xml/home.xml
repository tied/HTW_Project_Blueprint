<p><ac:structured-macro ac:name="excerpt-include" ac:schema-version="1" ac:macro-id="cd616eff-5c70-4251-b51c-793b4f97b98d"><ac:parameter ac:name=""><ac:link><ri:page ri:content-title="Projektausf&uuml;hrung" /></ac:link></ac:parameter></ac:structured-macro></p><ac:structured-macro ac:name="html" ac:schema-version="1" ac:macro-id="dc5b0a5c-6467-468e-bbef-19af996c9def"><ac:plain-text-body><![CDATA[<script>
/**
 * This script checks via Rest API if a specified content property for this space exist.
 * if not (typically directly after the space is created) the content property is created 
 * and a redirect to the "Projekt Erfassen" page is executed.
 * 
 *  
 */
var correctSite = false;
var url = window.location;
var urlArray = url.pathname.split("/display/");
if (urlArray.length > 0) {
  var space = urlArray[1].split('/')[0];
  console.log("HTW Blueprint: current space =" + space);
  var baseUrl = url.protocol + "//" + url.host;;
  console.log(baseUrl);
  var restUrl = baseUrl + "/rest/api/space/" + space + "/property";
  correctSite = true;
}
else {
  console.log("HTW Blueprint: Redirect Script will not be executet. This is not a Project Homepage.");
}


AJS.toInit(function () {
  if (correctSite) {
    jQuery.ajax({
      type: "GET",
      url: restUrl,
      dataType: 'json',
      async: false,
      success: function (resultData) {
        if (checkIfVisited(resultData) == false) {
          console.log("HTW Blueprint: Project not initalized. Execute Redirect.");
          createContentProperty();
          redirect();
        }
        else {
          console.log("HTW Blueprint: No Redirect necessary. Project already initialized.");
        }

      },
      error: function (msg, url, line) {
        console.log("HTW Blueprint: Error while requesting space content property: msg = " + msg + ", url = " + url + ", line = " + line);
    }

    });
  }
});

function checkIfVisited(resultData) {
  var results = resultData["results"];
  var visited = false;
  for (var key in results) {
    if (results[key]["key"] == "htwSpaceProps" && results[key].value.spaceInitalized == "true") {
        visited = true;
    }
  }
  return visited;
}

function createContentProperty() {

  jQuery.ajax({
    type: "POST",
    url: restUrl,
    dataType: 'json',
    async: false,
    contentType: 'application/json',
    data: JSON.stringify({ "key": "htwSpaceProps", "value": { "spaceInitalized": "true" } }),
    success: function (resultData) {
      console.log("HTW Blueprint: update Data success:" + resultData)
    },
    error: function (msg, url, line) {
      console.log("HTW Blueprint: Error while requesting space content property: msg = " + msg + ", url = " + url + ", line = " + line);

    }

  });
}

function redirect() {
  var newUrl = baseUrl + "/display/" + space + "/b%29+Projekt+Erfassen";
  console.log("HTW Blueprint: redirect to " + newUrl);
  window.location = newUrl;
}
</script>]]></ac:plain-text-body></ac:structured-macro></ac:layout-cell><ac:layout-cell>
